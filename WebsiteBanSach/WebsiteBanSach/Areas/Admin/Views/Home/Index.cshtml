@functions{
    int GetIso8601WeekOfYear(DateTime time)
    {
        // Seriously cheat.  If its Monday, Tuesday or Wednesday, then it'll
        // be the same week# as whatever Thursday, Friday or Saturday are,
        // and we always get those right
        DayOfWeek day = CultureInfo.InvariantCulture.Calendar.GetDayOfWeek(time);
        if (day >= DayOfWeek.Monday && day <= DayOfWeek.Wednesday)
        {
            time = time.AddDays(3);
        }

        // Return the week of our adjusted day
        return CultureInfo.InvariantCulture.Calendar.GetWeekOfYear(time, CalendarWeekRule.FirstFourDayWeek, DayOfWeek.Monday);
    }
}

@{
    int count = 0;
    foreach (var item in ViewBag.userThisWeek)
    {
        count += 1;
    }

    int count_customerThisweek = 0;
    int count_customerLastweek = 0;
    int diff_cus = 0;
    DateTime a = DateTime.Now;

    foreach (var item in ViewBag.userThisWeek)
    {
        int t1 = GetIso8601WeekOfYear(item.CreatedDate);
        int t2 = GetIso8601WeekOfYear(a);
        if (t1 == t2)
        {
            count_customerThisweek += 1;
        }

    }
    foreach (var item in ViewBag.userLastWeek)
    {
        int t1 = GetIso8601WeekOfYear(item.CreatedDate);
        int t2 = GetIso8601WeekOfYear(a);
        if (t2 - t1 == 1)
        {
            count_customerLastweek += 1;
        }
    }
    diff_cus = count_customerThisweek - count_customerLastweek;

    int count_odneedupdateThisweek = 0;
    int count_odneedupdateLastweek = 0;
    int diff_newodneedupdate = 0;
    foreach (var od in ViewBag.orderlist_needupdateThisWeek)
    {
        int t1 = GetIso8601WeekOfYear(od.CreatedDate);
        int t2 = GetIso8601WeekOfYear(a);
        if (t1 == t2)
        {
            count_odneedupdateThisweek += 1;
        }
    }
    foreach (var od in ViewBag.orderlist_needupdateLastWeek)
    {
        int t1 = GetIso8601WeekOfYear(od.CreatedDate);
        int t2 = GetIso8601WeekOfYear(a);
        if (t2 - t1 == 1)
        {
            count_odneedupdateLastweek += 1;
        }
    }
    diff_newodneedupdate = count_odneedupdateThisweek - count_odneedupdateLastweek;

    float total1_true = 0;
    float total_book1_true = 0;
    float total2_true = 0;
    float total_book2_true = 0;
    foreach (var item in ViewBag.orderLastWeek)
    {
        int t1 = GetIso8601WeekOfYear(item.CreatedDate);
        int t2 = GetIso8601WeekOfYear(a);
        if (t2 - t1 == 1)
        {
            foreach (var sanpham in ViewBag.Orderdetail)
            {
                if (sanpham.OrderID == item.ID)
                {
                    total_book1_true += sanpham.Number * float.Parse(sanpham.Price.ToString());
                }
            }
            total1_true = total_book1_true;
        }

    }
    foreach (var item in ViewBag.orderThisWeek)
    {
        int t1 = GetIso8601WeekOfYear(item.CreatedDate);
        int t2 = GetIso8601WeekOfYear(a);
        if (t2 == t1)
        {
            foreach (var sanpham in ViewBag.Orderdetail)
            {
                if (sanpham.OrderID == item.ID)
                {
                    total_book2_true += sanpham.Number * float.Parse(sanpham.Price.ToString());
                }
            }
            total2_true = total_book2_true;
        }
    }
    float total1 = total1_true;
    float total2 = total2_true;
    float differtotal = total2 - total1;

    int countbsoldoutThisweek = 0;
    int countbsoldoutLastweek = 0;
    int diff_countbsoldout = 0;
    if (ViewBag.BookSOThisWeek != null)
    {
        foreach (var item in ViewBag.BookSOThisWeek)
        {
            int t1 = GetIso8601WeekOfYear(item.CreatedDate);
            int t2 = GetIso8601WeekOfYear(a);
            if (t2 == t1)
            {
                countbsoldoutThisweek += 1;
            }
        }
    }
    if (ViewBag.BookSOLastWeek != null)
    {
        foreach (var item in ViewBag.BookSOThisWeek)
        {
            int t1 = GetIso8601WeekOfYear(item.CreatedDate);
            int t2 = GetIso8601WeekOfYear(a);
            if (t2 - t1 == 1)
            {
                countbsoldoutLastweek += 1;
            }
        }
    }
    diff_countbsoldout = countbsoldoutThisweek - countbsoldoutLastweek;
}

@helper Truncate(string input, int length)
{
                    /**/

                    if (input.Length < length)
                    {
                        @Html.Raw(input)
                    }
                    else
                    {
                        @Html.Raw(input.Substring(0, length))<text >...</text>
                    }
}
